{
    "name": "iak/dispatch-group",
    "description": "Cool stuff",
    "keywords": [
        "iaK",
        "dispatch-group"
    ],
    "homepage": "https://github.com/iak/dispatch-group",
    "license": "MIT",
    "authors": [{
        "name": "Isak Berglind",
        "email": "info@isakberglind.se",
        "homepage": "http://landslide-design.se",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.4",
    },
    "require-dev": {
        "laravel/horizon": "^4.3",
        "predis/predis": "^1.1",
        "friendsofphp/php-cs-fixer": "^2.16",
        "symfony/var-dumper": "^5",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Iak\\DispatchGroup\\": "src"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Iak\\DispatchGroup\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iak\\DispatchGroup\\DispatchGroupServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}